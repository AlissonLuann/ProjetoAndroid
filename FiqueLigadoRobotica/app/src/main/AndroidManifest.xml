<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.fiqueligadorobotica">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_roboticalogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HistoriaActivity"
            android:label="@string/title_activity_sub"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.fiqueligadorobotica.MainActivity" />
        </activity>
        <activity
            android:name=".CompeticoesActivity"
            android:label="@string/title_activity_terceira"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".RoboEletronicoActivity"
            android:label="@string/title_activity_quarta"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ProjetoArduinoActivity"
            android:label="@string/title_activity_quinta"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".AprenderRoboActivity"
            android:label="@string/title_activity_sexta"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
